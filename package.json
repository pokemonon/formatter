{
  "name": "formatter",
  "private": true,
  "scripts": {
    "clean": "lerna clean -y && rm -rf node_modules",
    "dev:docs": "vitedoc dev",
    "build:docs": "vitedoc build --outDir dist",
    "bump": "HUSKY=0 changeset && HUSKY=0 changeset version && pnpm install",
    "release": "changeset publish",
    "husky": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.0",
    "@vitesite/vitedoc-core": "^0.0.0-beta.10",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "lerna": "^3.21.0",
    "standard-version": "^9.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
